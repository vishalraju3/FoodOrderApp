    <section id="first-section">
      <div class="container">
        <div class="row">
          <div class="col-md-6">
            <div class="card">
              <img src="/Images/ViewProducts2.jpg" />
              <div class="card-bottom">
                <a class="h4 m-0 mt-2 text-center nav-link" href="/User/ProductView">View Products</a>
              </div>
            </div>
          </div>
          <div class="col-md-6">
            <div class="card">
              <img src="/Images/AddProduct.jpg" />
              <div class="card-bottom">
                <a class="h4 m-0 mt-2 text-center nav-link" src="#" href="/User/AddProduct">
                  Add Products
                </a>
              </div>
            </div>
          </div>
          <div class="col-md-6">
            <div class="card">
              <img src="/Images/DeleteProduct.jpg" />
              <div class="card-bottom">
                <a class="h4 m-0 mt-2 text-center nav-link" src="#" href="/User/DeleteProduct">
                  Delete Products
                </a>
              </div>
            </div>
          </div>
          <div class="col-md-6">
            <div class="card">
              <img src="/Images/UpdateProduct.jpeg" />
              <div class="card-bottom">
                <a class="h4 m-0 mt-2 text-center nav-link" src="#" href="/User/UpdateProduct">
